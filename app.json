{
  "expo": {
    "name": "Servicars",
    "slug": "servicars",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoicono.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#feedbe"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.servicars.servicars",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoicono.png",
        "backgroundColor": "#feedbe"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
